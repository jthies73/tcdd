<h1>New CleanUp</h1>  
<%= form_with model: [:admin, @clean_up], local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.submit "Create CleanUp" %>
  </div>
<% end %>